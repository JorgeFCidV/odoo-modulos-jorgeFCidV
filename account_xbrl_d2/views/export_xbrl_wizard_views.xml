<odoo>
    <record id="view_export_xbrl_wizard_form" model="ir.ui.view">
        <field name="name">export.xbrl.wizard.form</field>
        <field name="model">export.xbrl.wizard</field>
        <field name="arch" type="xml">
            <form string="Exportar XBRL">
                <group>
                    <field name="fiscal_year_id"/>
                </group>
                <footer>
                    <button string="Exportar" type="object" name="action_export_xbrl" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
