<odoo>
  <record id="view_export_attachment_wizard_form" model="ir.ui.view">
    <field name="name">export.attachment.wizard.form</field>
    <field name="model">export.attachment.wizard</field>
    <field name="arch" type="xml">
      <form string="Exportar adjuntos de facturas">
        <group>
          <field name="partner_id"/>
          <field name="date_from"/>
          <field name="date_to"/>
        </group>
        <footer>
          <button name="action_export" type="object" string="Exportar" class="btn-primary"/>
          <button string="Cancelar" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
